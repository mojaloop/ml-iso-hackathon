{
  "name": "@mojaloop-iso-hackathon/service-globaldiscovery",
  "version": "0.1.0",
  "description": "mojaloop iso hackathon - Global Discovery Service",
  "license": "Apache-2.0",
  "homepage": "https://github.com/mojaloop/ml-iso-hackathon",
  "bugs": {
    "url": "https://github.com/mojaloop/ml-iso-hackathon/issues"
  },
  "author": "Mojaloop ISO Hackathon Team",
  "contributors": [
    "Miguel de Barros <miguel.debarros@modusbox.com>"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "pre-commit": [
    "dep:check",
    "build",
    "lint",
    "test:pre-commit"
  ],
  "scripts": {
    "start": "npm run start:api",
    "start:api": "tsc && node dist/index.js api --config ../../default.env",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -Rf dist",
    "pretest": "npm run build",
    "standard": "npm run lint",
    "lint": "npm run build && npx eslint ./src",
    "lint:fix": "npm run build && npx eslint ./src --fix",
    "test:pre-commit-test": "echo 'no tests yet'",
    "test:unit": "jest --testMatch '**/test/unit/**/*.test.ts'",
    "test:coverage": "jest --coverage --coverageThreshold='{}' --testMatch '**/test/unit/**/*.test.ts'",
    "test:coverage-check": "jest --coverage --testMatch '**/test/unit/**/*.test.ts'",
    "audit:resolve": "SHELL=sh resolve-audit",
    "audit:check": "SHELL=sh check-audit",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u"
  },
  "dependencies": {
    "@mojaloop-iso-hackathon/lib-shared": "0.1.0",
    "@types/dotenv": "^8.2.0",
    "commander": "^7.1.0",
    "dotenv": "^8.2.0",
    "jsonpath-plus": "^5.0.4",
    "path": "^0.12.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "faucet": "0.0.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "npm-audit-resolver": "^2.2.1",
    "npm-check-updates": "^11.3.0",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "ts-jest": "^26.5.3",
    "typedoc": "^0.20.32",
    "typescript": "^4.2.3"
  }
}
